<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/trFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/trFragment"
        android:name="com.example.youtube.ui.youtube.tr.TrFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_tr" >
        <action
            android:id="@+id/action_youtubeFragment_to_usFragment"
            app:destination="@id/usFragment" />
        <action
            android:id="@+id/action_youtubeFragment_to_deFragment"
            app:destination="@id/deFragment" />
        <action
            android:id="@+id/action_trFragment_to_videoDetailFragment"
            app:destination="@id/videoDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/deFragment"
        android:name="com.example.youtube.ui.youtube.de.DeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_de" >
        <action
            android:id="@+id/action_deFragment_to_videoDetailFragment"
            app:destination="@id/videoDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/usFragment"
        android:name="com.example.youtube.ui.youtube.us.UsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_us" >
        <action
            android:id="@+id/action_usFragment_to_videoDetailFragment"
            app:destination="@id/videoDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/videoDetailFragment"
        android:name="com.example.youtube.ui.youtube.detail.VideoDetailFragment"
        android:label="@string/detail_title"
        tools:layout="@layout/fragment_video_detail" >
        <argument
            android:name="detailItem"
            app:argType="com.example.youtube.model.entities.Item" />
    </fragment>
</navigation>